{
    "name": "symfomany/laravelcinema",
    "description": "Pedagogic Project",
    "keywords": ["framework", "laravel", "mongodb"],
    "license": "MIT",
    "type": "project",
    "version": "1.0",
    "homepage": "https://github.com/Symfomany/laravelcinema",
    "authors": [
        {
            "name": "Boyer Julien",
            "email": "julien@meetserious.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "julien@meetserious.com",
        "irc": "irc://irc.freenode.org/composer"
    },

    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "predis/predis": "*",
        "caouecs/laravel4-lang": "^2.0",
        "laravel/envoy": "~1.0",
        "laravelrus/localized-carbon": "*",
        "laravel/socialite": "^2.0",
        "symfony/dom-crawler": "^2.7",
        "guzzlehttp/guzzle": "^6.1",
        "symfony/css-selector": "^2.7",
        "fabpot/goutte": "^3.1",
        "alaouy/youtube": "^1.2",
        "thujohn/twitter": "^2.0",
        "cviebrock/eloquent-sluggable": "^3.0",
        "barryvdh/laravel-debugbar": "^2.2@dev",
        "netshell/paypal": "dev-master",
        "jenssegers/mongodb": "^2.2",
        "phpdocumentor/phpdocumentor": "^2.8"

    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "orchestra/testbench": "^3.1",
        "benconstable/phpspec-laravel": "^2.0",
        "symfony/phpunit-bridge": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    }
}
